        <label for="cEmail" class="col text-center form-label">Email</label>
        <input class="form-control col text-center" type="email" name="cEmail" placeholder=""
               id="cEmail">
    </div>
    <div class="form-group">
        <label for="cCode" class="col text-center form-label">Confirmation Code</label>
        <input class="form-control col text-center" type="text" name="cCode" placeholder=""
               id="cCode">
    </div>
    <div class="col text-center">
        <button class="btn btn-primary" id="confSubmit">Submit</button>
    </div>
</form>
